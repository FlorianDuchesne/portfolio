<!DOCTYPE html>
<html>
	<head>
		<script src="https://kit.fontawesome.com/e2a8fec256.js" crossorigin="anonymous"></script>

		<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@3/dist/email.min.js"></script>
		<script src="https://www.google.com/recaptcha/api.js" async defer></script>

		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1" />
		<title>
			{% block title %}Chaise Vivante
			{% endblock %}
		</title>
		<meta name="description" content="{% block meta_description %} Chaise Vivante est le domaine de Florian Duchesne {% endblock %}">
		{% block icon %} <link rel="icon" href= {{ asset('/icones/icone_portfolio.png') }}>	{% endblock %}
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% block body %}
		{% endblock %}
	</body>
</html>
